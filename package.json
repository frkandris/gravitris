{
  "name": "gravitris",
  "version": "1.1.3",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/frkandris/gravitris"
  },
  "scripts": {
    "start": "npm run start-local",
    "start-local": "npm install && cross-env NODE_ENV=local nodemon ./app/index.js",
