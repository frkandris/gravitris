    "cross-env": "^6.0.3",
    "debug": "~2.6.9",
    "express": "~4.18.2",
    "express-handlebars": "^5.3.1",
    "fs": "0.0.1-security",
    "hbs": "^4.0.6",
    "http-errors": "~1.6.3",
    "moment": "^2.29.2",
    "mongodb": "^3.3.3",
    "mongoose": "^5.7.9",
    "morgan": "~1.9.1",
    "nconf": "^0.11.4",
    "promise": "^8.0.3"
  },
  "devDependencies": {
    "nodemon": "^1.19.4"
  }
