    "cross-env": "^6.0.0",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-handlebars": "^3.1.0",
    "fs": "0.0.1-security",
    "hbs": "^4.0.5",
    "http-errors": "~1.6.3",
    "moment": "^2.24.0",
    "mongodb": "^3.3.2",
    "mongoose": "^5.7.1",
    "morgan": "~1.9.1",
    "nconf": "^0.10.0",
    "promise": "^8.0.3"
  },
  "devDependencies": {
    "nodemon": "^1.19.2"
  }
